# macOS 优化配置
# 禁用代理，使用直连模式
USE_PROXY=false
FORCE_PROXY=false

# 网络优化配置
NODE_ENV=production

# 连接超时优化（macOS网络栈较严格）
REQUEST_TIMEOUT=15000
CONNECT_TIMEOUT=10000

# DNS优化
DNS_TIMEOUT=5000

# 禁用IPv6（避免双栈问题）
NODE_OPTIONS="--dns-result-order=ipv4first"

# 调试模式（可选）
DEBUG=false
VERBOSE_LOGGING=false

